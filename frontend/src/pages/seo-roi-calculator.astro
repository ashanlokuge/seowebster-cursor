           <script>
           // SEO ROI Calculator Functionality
           document.addEventListener('DOMContentLoaded', () => {
             const calculateBtn = document.getElementById('calculate-btn');
             const results = document.getElementById('results');
             const resultsCta = document.getElementById('results-cta');
             
             // Industry growth multipliers
             const industryMultipliers = {
               'ecommerce': { traffic: 2.5, conversion: 1.4, time: 8 },
               'saas': { traffic: 3.2, conversion: 1.6, time: 10 },
               'services': { traffic: 2.8, conversion: 1.5, time: 9 },
               'healthcare': { traffic: 2.2, conversion: 1.3, time: 12 },
               'finance': { traffic: 2.0, conversion: 1.2, time: 14 },
               'education': { traffic: 2.6, conversion: 1.4, time: 10 },
               'real-estate': { traffic: 2.4, conversion: 1.3, time: 11 },
               'other': { traffic: 2.5, conversion: 1.4, time: 10 }
             };

             calculateBtn.addEventListener('click', () => {
               // Get input values
               const monthlyRevenue = parseFloat(document.getElementById('monthly-revenue').value) || 0;
               const monthlyTraffic = parseFloat(document.getElementById('monthly-traffic').value) || 0;
               const conversionRate = parseFloat(document.getElementById('conversion-rate').value) || 0;
               const avgOrderValue = parseFloat(document.getElementById('avg-order-value').value) || 0;
               const seoInvestment = parseFloat(document.getElementById('seo-investment').value) || 0;
               const industry = document.getElementById('industry').value;

               if (monthlyRevenue === 0 || monthlyTraffic === 0 || conversionRate === 0 || avgOrderValue === 0 || seoInvestment === 0) {
                 alert('Please fill in all fields with valid numbers to calculate your SEO ROI.');
                 return;
               }

               // Get industry multipliers
               const multipliers = industryMultipliers[industry];

               // Calculate projections
               const trafficGrowth = Math.round(monthlyTraffic * (multipliers.traffic - 1));
               const newTraffic = monthlyTraffic + trafficGrowth;
               const newConversionRate = conversionRate * multipliers.conversion;
               const newConversions = (newTraffic * newConversionRate) / 100;
               const newRevenue = newConversions * avgOrderValue;
               const revenueIncrease = newRevenue - monthlyRevenue;
               const annualRevenueIncrease = revenueIncrease * 12;
               const roiPercentage = ((annualRevenueIncrease - (seoInvestment * 12)) / (seoInvestment * 12)) * 100;
               const paybackMonths = Math.ceil((seoInvestment * 12) / annualRevenueIncrease);

               // Display results
               document.getElementById('traffic-growth').textContent = `+${trafficGrowth.toLocaleString()}`;
               document.getElementById('traffic-growth').textContent = `+${trafficGrowth.toLocaleString()}`;
               document.getElementById('revenue-increase').textContent = `$${revenueIncrease.toLocaleString()}`;
               document.getElementById('roi-percentage').textContent = `${roiPercentage.toFixed(0)}%`;
               document.getElementById('payback-period').textContent = `${paybackMonths} months`;

               // Show results and CTA
               if (results) results.style.opacity = '1';
               if (resultsCta) resultsCta.classList.remove('hidden');

               // Smooth scroll to results
               if (results) results.scrollIntoView({ behavior: 'smooth', block: 'center' });
             });

             // Add input validation and real-time updates
             const inputs = document.querySelectorAll('input, select');
             inputs.forEach(input => {
               input.addEventListener('input', () => {
                 // Reset results when inputs change
                 if (results) results.style.opacity = '0.5';
                 if (resultsCta) resultsCta.classList.add('hidden');
               });
             });

             // FAQ Accordion Functionality
             const faqToggles = document.querySelectorAll('.faq-toggle');
             
             faqToggles.forEach((toggle) => {
               toggle.addEventListener('click', (event) => {
                 const target = event.currentTarget;
                 const content = target.nextElementSibling;
                 const icon = target.querySelector('svg');
                 const isExpanded = target.getAttribute('aria-expanded') === 'true';
                 
                 // Toggle the current FAQ item
                 if (isExpanded) {
                   // Collapse
                   content.classList.add('hidden');
                   target.setAttribute('aria-expanded', 'false');
                   icon.style.transform = 'rotate(0deg)';
                 } else {
                   // Expand
                   content.classList.remove('hidden');
                   target.setAttribute('aria-expanded', 'true');
                   icon.style.transform = 'rotate(180deg)';
                 }
               });
             });
           });
         </script>
